---
layout: nil
---

[
{% for field in site.data.order.brays %}
	{% assign element = site.data.elements[field] %}
	{% assign brays = site.data.brays[field] %}
	{"label": "{{ element.label }}","namespace": "{{ element.namespace }}","name": "{{ element.name }}","definition": "{{ element.definition }}","uri": "{{ site.url }}#{{ element.name }}","guidelines": "{{ site.url }}/guidelines/{{ element.name }}"{% if element.source %},"source": "{{ element.source }}"{% endif %}{% if element.crosswalk %},"crosswalk": {{% for c in element.crosswalk %}{{ c | jsonify }}{% if forloop.last == false %},{% endif %}{% endfor %}}{% endif %},"range": [{% for r in element.range %}{{ r | jsonify }}{% if forloop.last == false %},{% endif %}{% endfor %}],"obligation": "{{ element.obligation }}","type": "{{ element.type }}","repeatable": {{ element.repeatable }},"input": "{{ brays.input }}","visible": {{ brays.visible }},"editable": {{ brays.editable }}}{% if forloop.last == false %},{% endif %}
{% endfor %}
]
